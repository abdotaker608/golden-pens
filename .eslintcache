   }\r\n                setError(true);\r\n            })\r\n            .catch(() => setError(true))\r\n    }\r\n\r\n    useEffect(() => {\r\n        if (user) fetchStory();\r\n    }, [user])\r\n\r\n    if (!user) return <NotAllowed />\r\n\r\n    if (error || (story.author && user.pk !== story.author.user.pk)) return <Redirect to='/write/new' />\r\n\r\n    return (\r\n        <div className='new-story'>\r\n            <div className='main-builder position-relative'>\r\n                <CoverForm translate={translateWrapper} cover={cover} setCover={setCover}/>\r\n                <InfoForm translate={translateWrapper} lang={lang} cover={cover} user={user} story={story} cat={story.category} tagList={story.author ? story.tags : []} method='PUT'/>\r\n            </div>\r\n        </div>\r\n    )\r\n}\r\n\r\nexport default EditStory\r\n",["194","195"],"/media/eldababa/F8B2679AB2675C5E/Users/AG/Documents/React/golden-pens/src/components/Chapter/Delete.js",["196"],"/media/eldababa/F8B2679AB2675C5E/Users/AG/Documents/React/golden-pens/src/components/Save/Saved.js",["197"],"/media/eldababa/F8B2679AB2675C5E/Users/AG/Documents/React/golden-pens/src/components/Authors/AuthorCover.js",[],"/media/eldababa/F8B2679AB2675C5E/Users/AG/Documents/React/golden-pens/src/components/Exhibition/Config.js",[],"/media/eldababa/F8B2679AB2675C5E/Users/AG/Documents/React/golden-pens/src/components/ADS/ADS.js",[],"/media/eldababa/F8B2679AB2675C5E/Users/AG/Documents/React/golden-pens/src/components/Login/LoginForm.js",["198","199"],"/media/eldababa/F8B2679AB2675C5E/Users/AG/Documents/React/golden-pens/src/components/Write/Tag.js",[],"/media/eldababa/F8B2679AB2675C5E/Users/AG/Documents/React/golden-pens/src/components/Qm/Qm.js",["200"],"/media/eldababa/F8B2679AB2675C5E/Users/AG/Documents/React/golden-pens/src/components/Home/WritePreview.js",["201"],"/media/eldababa/F8B2679AB2675C5E/Users/AG/Documents/React/golden-pens/src/components/Comment/Comment.js",["202","203"],"import React, {useRef, useState} from 'react';\r\nimport moment from 'moment';\r\nimport ar from 'moment/locale/ar';\r\nimport DropDown from '../Dropdown/Dropdown';\r\nimport {useSelector} from 'react-redux';\r\nimport {translate} from '../../Js/methods';\r\nimport ReplyTE from '../TextEditor/ReplyTE';\r\nimport {endpoint} from '../../API';\r\nimport ProcessingButton from '../ProcessingButton/ProcessingButton';\r\nimport {Link} from 'react-router-dom';\r\n\r\n\r\nfunction Comment({comment, editComment, deleteComment}) {\r\n\r\n    const lang = useSelector(state => state.lang);\r\n    const user = useSelector(state => state.auth);\r\n\r\n    const [editContent, setEditContent] = useState(comment.content);\r\n    const [processing, setProcessing] = useState(false);\r\n\r\n    const editor = useRef(null);\r\n\r\n    const translateWrapper = text => translate(text, lang);\r\n\r\n    const getPicture = user => {\r\n        if (user.picture) return user.picture;\r\n        else if (user.social_picture) return user.social_picture;\r\n        return require('../../static/images/avatar.svg').default;\r\n    }\r\n\r\n    const deleteRequest = () => {\r\n        if (processing) return;\r\n\r\n        setProcessing(true);\r\n        \r\n        fetch(`${endpoint}/stories/replies/update/${comment.id}`, {\r\n            headers: {\r\n                'Authorization': `Token ${user.token}`,\r\n            },\r\n            method: 'DELETE',\r\n        })\r\n            .then(res => {\r\n                setProcessing(false);\r\n                if (res.status === 204) deleteComment(comment.id);\r\n            })\r\n            .catch(() => setProcessing(false));\r\n    }\r\n\r\n    const btns = [\r\n        {name: translateWrapper('edit'), icon: 'fas fa-edit', onClick: () => editComment(comment.id)},\r\n        {name: translateWrapper('delete'), icon: 'fas fa-trash', onClick: deleteRequest}\r\n    ]\r\n\r\n    const btnElems = btns.map(btn => (\r\n        <button onClick={btn.onClick} key={btn.name}><span className='mx-2'><i className={btn.icon} /></span>{btn.name}</button>\r\n    ))\r\n\r\n    const handleChange = content => setEditContent(content);\r\n\r\n    const saveChanges = () => {\r\n        if (processing) return;\r\n\r\n        setProcessing(true);\r\n\r\n        fetch(`${endpoint}/stories/replies/update/${comment.id}`, {\r\n            headers: {\r\n                'Authorization': `Token ${user.token}`,\r\n                'Content-Type': 'application/json'\r\n            },\r\n            method: 'PUT',\r\n            body: JSON.stringify({content: editContent})\r\n        })\r\n            .then(res => res.json())\r\n            .then(data => {\r\n                setProcessing(false);\r\n                if (data.id) {\r\n                    editComment(comment.id, false, data.content);\r\n                }\r\n            })\r\n            .catch(() => {\r\n                setProcessing(false);\r\n            })\r\n    }\r\n\r\n\r\n    return (\r\n        <div className='comment'>\r\n            {\r\n                comment.editing ?\r\n                <div className='editing-container'>\r\n                    <ReplyTE initialValue={comment.content} getEditor={editor} onEditorChange={handleChange} />\r\n                    <div className='d-flex justify-content-center align-items-center mt-3'>\r\n                        <ProcessingButton onClick={saveChanges} processing={processing}>{translateWrapper('post')}</ProcessingButton>\r\n                        <button onClick={() => editComment(comment.id, false)} className='cancel-btn'>{translateWrapper('cancel')}</button>\r\n                    </div>\r\n                </div>\r\n                :\r\n                <React.Fragment>\r\n                    <div className='d-flex justify-content-between align-items-center'>\r\n                        <div className='user d-flex'>\r\n                            <img src={getPicture(comment.user)} />\r\n                            <div className='mx-3 mt-2'>\r\n                                <Link to={`/profile/${comment.user.pk}`}>\r\n                                    <h5 className='text-capitalize font-weight-bold' dir='auto'>{comment.user.fullname}</h5>\r\n                                </Link>\r\n                                <h6 className='text-capitalize text-secondary'>{moment(comment.created).locale(lang).fromNow()}</h6>\r\n                            </div>\r\n                        </div>\r\n                        {user && comment.user.pk === user.pk && <DropDown items={btnElems} />}\r\n                    </div>\r\n                    <p dir='auto' dangerouslySetInnerHTML={{__html: comment.content}}></p>\r\n                </React.Fragment>\r\n            }\r\n\r\n        </div>\r\n    )\r\n}\r\n\r\nexport default Comment\r\n","/media/eldababa/F8B2679AB2675C5E/Users/AG/Documents/React/golden-pens/src/components/LanguageWidget/LanguageWidget.js",[],"/media/eldababa/F8B2679AB2675C5E/Users/AG/Documents/React/golden-pens/src/components/Dashboard/Trending.js",[],"/media/eldababa/F8B2679AB2675C5E/Users/AG/Documents/React/golden-pens/src/components/ProcessingButton/ProcessingButton.js",[],"/media/eldababa/F8B2679AB2675C5E/Users/AG/Documents/React/golden-pens/src/components/Home/WritersHome.js",[],"/media/eldababa/F8B2679AB2675C5E/Users/AG/Documents/React/golden-pens/src/components/Dashboard/SA.js",[],"/media/eldababa/F8B2679AB2675C5E/Users/AG/Documents/React/golden-pens/src/components/Story/TableOfContents.js",["204"],"import React, {useEffect, useState} from 'react';\r\nimport {Link} from 'react-router-dom';\r\nimport Dropdown from '../Dropdown/Dropdown';\r\nimport FetchPreloader from '../FetchPreloader/FetchPreloader';\r\nimport {endpoint} from '../../API';\r\n\r\n\r\nfunction TableOfContents({story, translate, user}) {\r\n\r\n    const [chapters, setChapters] = useState(null);\r\n    const [error, setError] = useState('');\r\n    \r\n    const fetchChapters = () => {\r\n        fetch(`${endpoint}/stories/overview/chapters/${story.id}`)\r\n            .then(res => res.json())\r\n            .then(data => {\r\n                if (data.length) {\r\n                    setChapters(data);\r\n                }\r\n                else{\r\n                    setChapters([]);\r\n                    setError('failFetch');\r\n                }\r\n            })\r\n            .catch(() => setError('wentWrong'));\r\n    }\r\n\r\n    useEffect(() => {\r\n        fetchChapters();\r\n    }, [])\r\n\r\n    const links = [\r\n        {name: translate('newChapter'), icon: 'fas fa-plus fa-lg', path: `/write/nc/${story.id}`},\r\n        {name: translate('editStory'), icon: 'fas fa-edit fa-lg', path: `/edit/story/${story.id}`},\r\n        {name: translate('deleteStory'), icon: 'fas fa-trash fa-lg', path: `/delete/story/${story.id}`},\r\n        {name: translate('save'), icon: 'fas fa-download fa-lg', path: `/save/${story.id}`}\r\n    ]\r\n\r\n    const eLinks = [\r\n        {name: translate('report'), icon: 'fas fa-flag fa-lg', path: `/report/${story.id}`},\r\n        {name: translate('save'), icon: 'fas fa-download fa-lg', path: `/save/${story.id}`}\r\n    ]\r\n\r\n    const linksElems = links.map(link => (\r\n        <Link className='item' to={link.path} key={link.path}><span><i className={link.icon} /></span> <span className='mx-2'>{link.name}</span></Link>\r\n    ))\r\n\r\n    const reportElems = eLinks.map(link => (\r\n        <Link className='item' to={link.path} key={link.path}><span><i className={link.icon} /></span> <span className='mx-2'>{link.name}</span></Link>\r\n    ))\r\n\r\n    const loggedOutElems = eLinks.slice(1).map(link => (\r\n        <Link className='item' to={link.path} key={link.path}><span><i className={link.icon} /></span> <span className='mx-2'>{link.name}</span></Link>\r\n    ))\r\n\r\n    return (\r\n        <div className='contents-table'>\r\n            <div className='table-header'>\r\n                <h4>{translate('tableOfContents')}</h4>\r\n                {\r\n                    user ?\r\n                    <React.Fragment>\r\n                        {\r\n                            user.pk === story.author.user.pk ?\r\n                            <Dropdown items={linksElems} />:\r\n                            <Dropdown items={reportElems} />\r\n                        }\r\n                    </React.Fragment>\r\n                    :\r\n                    <Dropdown items={loggedOutElems} />\r\n                }\r\n            </div>\r\n            {\r\n                    chapters ?\r\n                    <div className='chapters-container d-flex flex-column'>\r\n                        {\r\n                            chapters.length === 0 ?\r\n                            <div className='py-5 text-center text-secondary'>\r\n                                <p>{translate('noDesc')}</p>\r\n                            </div>\r\n                            :\r\n                            chapters.map(chapter => (\r\n                                    <Link to={`/story/c/${chapter.pk}`} className='text-capitalize' key={chapter.pk}>\r\n                                        <div className='chapter' key={chapter.pk} dir='auto'>{chapter.title}</div>\r\n                                    </Link>\r\n                            ))\r\n                        }\r\n                    </div>\r\n                    :\r\n                    <FetchPreloader error={translate(error)}/>\r\n            }\r\n        </div>\r\n    )\r\n}\r\n\r\nexport default TableOfContents\r\n","/media/eldababa/F8B2679AB2675C5E/Users/AG/Documents/React/golden-pens/src/components/Home/BecomeAuthor.js",["205"],"/media/eldababa/F8B2679AB2675C5E/Users/AG/Documents/React/golden-pens/src/reportWebVitals.js",[],"/media/eldababa/F8B2679AB2675C5E/Users/AG/Documents/React/golden-pens/src/components/Home/Home.js",[],"/media/eldababa/F8B2679AB2675C5E/Users/AG/Documents/React/golden-pens/src/components/Settings/Settings.js",[],"/media/eldababa/F8B2679AB2675C5E/U